{
  "name": "arcticfox-config",
  "version": "0.9.0-beta.5",
  "private": true,
  "description": "Configuration Tool for Vape Battery Mods with Arcticfox Firmware. Works on MacOS and Linux.",
  "main": "main.js",
  "repository": "https://github.com/hobbyquaker/arcticfox-config",
  "keywords": [
    "vape",
    "ecigarette",
    "evolv",
    "arcticfox",
    "config",
    "battery",
    "mod",
    "nfeteam",
    "ntoolbox"
  ],
  "author": {
    "name": "Sebastian Raff",
    "email": "bastiraff@googlemail.com",
    "url": "https://github.com/hobbyquaker"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.11.1",
    "electron-rebuild": "^1.5.11"
  },
  "dependencies": {
    "arcticfox": "^9.0.0",
    "async": "^2.1.4",
    "electron-is-dev": "^0.2.0",
    "electron-json-storage": "^3.0.0",
    "electron-window-state": "^4.0.1",
    "highcharts": "^5.0.12",
    "highcharts-draggable-points": "^2.0.5",
    "jquery": "^3.1.1",
    "photonkit": "^0.1.2"
  },
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Arcticfox Config",
    "files": [
      "index.html",
      "main.js",
      "renderer.js",
      "pireg.html",
      "renderer-pireg.js",
      "tfr.html",
      "renderer-tfr.js",
      "power.html",
      "renderer-power.js",
      "style.css",
      "node_modules/",
      "i18n/"
    ],
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz"
      ]
    }
  }
}
