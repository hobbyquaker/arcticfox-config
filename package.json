{
  "name": "arcticfox-config",
  "version": "1.11.4",
  "private": true,
  "description": "Configuration Tool for Vape Battery Mods with Arcticfox Firmware. Works on MacOS and Linux.",
  "main": "main.js",
  "repository": "https://github.com/hobbyquaker/arcticfox-config",
  "keywords": [
    "vape",
    "ecigarette",
    "evolv",
    "arcticfox",
    "config",
    "battery",
    "mod",
    "nfeteam",
    "ntoolbox"
  ],
  "author": {
    "name": "Sebastian Raff",
    "email": "bastiraff@googlemail.com",
    "url": "https://github.com/hobbyquaker"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-builder": "^19.27.7",
    "electron-rebuild": "^1.6.0"
  },
  "dependencies": {
    "arcticfox": "^11.0.2",
    "arcticfox-encryption": "^1.0.1",
    "async": "^2.1.4",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^3.0.0",
    "electron-window-state": "^4.0.1",
    "highcharts": "^5.0.12",
    "highcharts-draggable-points": "^2.0.5",
    "jquery": "^3.1.1",
    "photonkit": "^0.1.2",
    "xml2js": "^0.4.17"
  },
  "scripts": {
    "rebuild": "electron-rebuild -f",
    "start": "electron .",
    "dist": "build"
  },
  "build": {
    "productName": "Arcticfox Config",
    "files": [
      "index.html",
      "main.js",
      "renderer.js",
      "pireg.html",
      "renderer-pireg.js",
      "tfr.html",
      "renderer-tfr.js",
      "power.html",
      "renderer-power.js",
      "bat.html",
      "renderer-bat.js",
      "style.css",
      "afcfile.js",
      "default.afc.json",
      "icon.png",
      "node_modules/",
      "i18n/"
    ],
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz",
        "rpm"
      ]
    },
    "deb": {
      "afterInstall": "build/after-install.tpl"
    },
    "rpm": {
      "afterInstall": "build/after-install.tpl"
    }
  }
}
